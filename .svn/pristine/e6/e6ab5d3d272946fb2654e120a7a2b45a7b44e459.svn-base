*{
    font-family: 'Hiragino Sans GB','PingFangSC-Regular', sans-serif;
    -webkit-tap-highlight-color:transparent;
    box-sizing: border-box;
}
body{
    width:100%;
    overflow-x: hidden;
    background: #eee;
}
.top-title-pro{
    text-align: center;
    height:1.28rem;
    padding-top:0.2rem;
    line-height: 1.08rem;
    font-size:0.36rem;
    font-weight:bold;
    background: white;
}
.pro-banner{
    width:100%;
    height:4.60rem;
    &>img{
        width: 100%;
        height:4.60rem;
    }
}
.pro-desc{
    background: white;
    padding:0.4rem 0 0 0.3rem;
    margin-bottom:0.2rem;
    .pro-desc-title{
        display: flex;
        .pro-desc-title-name{
            flex:1;
            font-size:0.36rem;
            color:#444;
            line-height: 0.36rem;
        }
        .averagePrice{
            flex:0 0 3rem;
            width:3rem;
            text-align: right;
            font-size: 0.22rem;
            color:#ccc;
            margin-right:0.3rem;
        }
    }
    .pro-desc-tage{
        margin-top:0.24rem;
        &>span{
            display:inline-block;
            padding:3px;
            background:#F0F6EC;
            color: #6FB6F4;
            font-size:0.24rem;
            line-height: 0.24rem;
        }
    }
    .totalcount{
        color:#F4661E;
        font-size:0.36rem;
        font-weight:bold;
        padding-bottom:0.3rem;
        border-bottom:1px solid #f5f5f5;
    }
    .pro-desc-address{
        padding:0.25rem 0;
        border-bottom:1px solid #f5f5f5;
        display: flex;
        &.last{
            border:none;
        }
        .pro-desc-address-left{
            display: inline-block;
            flex:0 0 0.65rem;
            width:0.65rem;
            color:#999;
            font-size:0.28rem;
            margin-right:0.24rem;
        }
        .pro-desc-address-right{
            display: inline-block;
            flex:1;
            color:#444444;
            font-size:0.28rem;
        }
    }
}
.project-dynamic{
    padding:0.4rem 0 0.3rem 0.3rem ;
    margin-bottom:0.2rem;
    background: white;
    .dynamic-title{
        color: #444;
        font-size:0.3rem;
        line-height:0.3rem;
        font-weight:bold;
        padding-bottom:0.4rem;
        border-bottom:1px solid #f5f5f5;
    }
    .dynamic-detail{
        color:#666;
        font-size:0.24rem;
        padding-top:0.4rem;
        padding-right:0.3rem;
    }
    .jianjie-detail{
        color:#666;
        font-size:0.26rem;
        padding-top:0.4rem;
        padding-right:0.3rem;
        overflow: hidden;
        text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 5;//（行数）
        -webkit-box-orient: vertical;
    }
    .showmore{
        background: transparent;
        border:none;
        text-align: center;
        outline: none;
        width:1.2rem;
        padding:0;
        color:#0385DB;
        display: block;
        margin:0.3rem auto 0rem;
        font-size:0.28rem;
    }
}
.main-type{
    background: white;
    padding:0.4rem 0;
    margin-bottom:0.2rem;
    .main-type-title{
        padding-left:0.3rem;
        color: #444444;
        font-size:0.3rem;
        padding-bottom:0.2rem;
        border-bottom:1px solid #f5f5f5;
    }
    .swiper-container2{
        padding:0.3rem;
        padding-bottom:0;
       .swiper-wrapper{
           min-height:2.56rem;
           .swiper-slide{
               border:1px solid #CCCCCC;
               &>img{
                   width:2.6rem;
                   height:2.56rem;
               }
               .type-condition{
                   display: flex;
                   color:#444;
                   font-size:0.26rem;
                   padding-left:5px;
                   padding-top:0.15rem;
                   .ting{
                       flex:1;
                   }
                   .sou{
                       flex:0 0 0.6rem;
                       width:0.6rem;
                   }
               }
                .square{
                   color:#999;
                   font-size:0.24rem;
                   padding-left:5px;
               }
               .price{
                color:#FF6972;
                // text-align: center;
                font-size:0.24rem;
                padding-bottom:0.2rem;
                padding-left:5px;
               }
           }
       }
    }
}
.location{
    background: white;
    padding-top:0.4rem;
    &>h3{
        padding-left:0.3rem;
        padding-bottom:0.3rem;
        margin-bottom:0.4rem;
        color:#444;
        font-size: 0.3rem;
        border-bottom:1px solid #f5f5f5;
    }
    #map{
        height:3.5rem;
    }
    .traffic{
        .traffic-tabs{
            height:0.88rem;
            line-height: 0.88rem;
            display: flex;
            padding:0 0.2rem;
            border-bottom:1px solid #f5f5f5;
            .tab-item{
                flex:1;
                text-align: center;
                color:#999999;
                font-size:0.3rem;
                border-bottom:2px solid transparent;
                &.on{
                    color:#0484DB;
                    border-bottom:2px solid #0484DB;
                }
            }
        }
        .traffic-details{
            background: white;
            padding:0.5rem 0.3rem 0;
            display: none;
            &.on{
                display: block;  
            }
            &>div{
                &>div{
                    display: flex;
                    padding-bottom:0.5rem;
                    .detail{
                        flex:1;
                        color:#666666;
                        font-size:0.28rem;
                        overflow: hidden;
                        text-overflow: ellipsis;
                        white-space: nowrap;

                    }
                    .distance{
                        flex:0 0 1rem;
                        width:1rem;
                        text-align: right;
                        font-size:0.26rem;
                        color:#999;
                    }
                }
                
            }
        }
    }
}